# fastlane 2.210.0

# The bundle identifier of your iOS application
app_identifier("com.phrsat.healthbridge")

# Your Apple ID credentials from environment variable for security
apple_id(ENV["APPLE_ID"])

# Your Apple Developer Portal Team ID from environment variable
team_id(ENV["TEAM_ID"])

# Your App Store Connect Team ID from environment variable
itc_team_id(ENV["ITC_TEAM_ID"])